{
  "$schema": "../../gen/schemas/desktop-schema.json",
  "identifier": "default-capability",
  "description": "Default capabilities for the application",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:webview:default",
    "opener:default",
    "store:default",
    "store:allow-set",
    "store:allow-get",
    "store:allow-delete",
    "store:allow-reset",
    "store:allow-keys",
    "store:allow-values",
    "store:allow-entries",
    "store:allow-length",
    "store:allow-load",
    "store:allow-save"
  ]
}
